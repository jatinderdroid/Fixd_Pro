<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_grey_blur"
    tools:context="fixtpro.com.fixtpro.AvailableJobListClickActivity">

    <android.support.v7.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:local="http://schemas.android.com/apk/res-auto"
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="@color/actionbar_bg"
        android:minHeight="?attr/actionBarSize"
        local:popupTheme="@style/ThemeOverlay.AppCompat.Light"
        local:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/cancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/screen_cross" />

            <TextView
                android:id="@+id/txtToolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/job_complete"
                android:textColor="@color/register_button_text"
                android:textSize="@dimen/largetext" />
        </RelativeLayout>
    </android.support.v7.widget.Toolbar>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/toolbar"
        android:layout_marginLeft="@dimen/main_left_right_margin"
        android:layout_marginRight="@dimen/main_left_right_margin">

        <LinearLayout
            android:id="@+id/layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/all_margintop"
            android:orientation="vertical">

            <!-- Add Title and Image Here In Horizontal Scroll View -->
        <fixtpro.com.fixtpro.views.HorizontalListView
            android:id="@+id/hlvImageList"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            >



        </fixtpro.com.fixtpro.views.HorizontalListView>
        </LinearLayout>
        <!--layout bottom line..-->
        <TextView
            android:id="@+id/line1"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_below="@+id/layout1"
            android:layout_marginTop="5dp"
            android:background="@color/lb_tv_white" />

        <LinearLayout
            android:id="@+id/layout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/line1"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txtName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/all_margintop"
                android:gravity="center"
                android:singleLine="true"
                android:text="testing"
                android:textColor="@color/lb_tv_white"
                android:textSize="@dimen/largetext" />

            <TextView
                android:id="@+id/txtAddress"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/all_margintop"
                android:gravity="center"
                android:singleLine="true"
                android:text="(null,null,null)"
                android:textColor="@color/lb_tv_white"
                android:textSize="@dimen/mediumtxts" />

            <TextView
                android:id="@+id/txtDateTime"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/all_margintop"
                android:gravity="center"
                android:singleLine="true"
                android:text="Wed,dec 30,2015"
                android:textColor="@color/lb_tv_white"
                android:textSize="@dimen/largetext" />

            <TextView
                android:id="@+id/txtArrivaltime"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/all_margintop"
                android:layout_weight="1"
                android:gravity="center"
                android:singleLine="true"
                android:text="Arrival Time:"
                android:textColor="@color/lb_tv_white"
                android:textSize="@dimen/mediumtxts" />

            <TextView
                android:id="@+id/txtCompletedTime"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/all_margintop"
                android:layout_weight="1"
                android:gravity="center"
                android:singleLine="true"
                android:text="Completed Time:"
                android:textColor="@color/lb_tv_white"
                android:textSize="@dimen/mediumtxts" />
        </LinearLayout>
        <!--layout bottom line..-->
        <TextView
            android:id="@+id/line2"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_below="@+id/layout2"
            android:layout_marginTop="5dp"
            android:background="@color/lb_tv_white" />

        <LinearLayout
            android:id="@+id/layout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/line2"
            android:layout_marginTop="@dimen/all_margintop"
            android:orientation="horizontal">

            <de.hdodenhof.circleimageview.CircleImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                android:id="@+id/circleImage"
                android:layout_width="@dimen/imgheiwid"
                android:layout_height="@dimen/imgheiwid"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/all_margintop"
                android:src="@drawable/addphoto_img"
                app:civ_border_color="#ffffff" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:gravity="left"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtTechName"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/all_margintop"
                    android:singleLine="true"
                    android:text="Georges Erre"
                    android:textColor="@color/lb_tv_white"
                    android:textSize="@dimen/mediumtxts" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="@dimen/all_margintop"
                    android:orientation="horizontal">
                    <fixtpro.com.fixtpro.views.RatingBarView
                        android:id="@+id/custom_ratingbar_tech"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:starCount="5"
                        app:starEmpty="@drawable/blank_star"
                        app:starFill="@drawable/fill_star"

                        app:starImageSize="@dimen/jobcompleteratesize" />
                </LinearLayout>


                <TextView
                    android:id="@+id/txtTotalJobScheduled"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/all_margintop"
                    android:singleLine="true"
                    android:text="Georges has 23 jobs scheduled forsdfs"
                    android:textColor="@color/lb_tv_white"
                    android:textSize="11sp" />
            </LinearLayout>
        </LinearLayout>
        <!--layout bottom line..-->
        <TextView
            android:id="@+id/line3"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_below="@+id/layout3"
            android:layout_marginTop="5dp"
            android:background="@color/lb_tv_white" />

        <LinearLayout
            android:id="@+id/layout4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/line3"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txt8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/all_margintop"
                android:gravity="center"
                android:singleLine="true"
                android:text="Job Total"
                android:textColor="@color/lb_tv_white"
                android:textSize="@dimen/largetext" />

            <TextView
                android:id="@+id/txtTotal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/all_margintop"
                android:gravity="center"
                android:singleLine="true"
                android:text="Total..............$299"
                android:textColor="@color/lb_tv_white"
                android:textSize="@dimen/largetext" />

            <TextView
                android:id="@+id/txtServceTicket"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/all_margintop"
                android:background="@drawable/white_rectangle"
                android:gravity="center"
                android:paddingBottom="5dp"
                android:paddingLeft="@dimen/service_btn_padding"
                android:paddingRight="@dimen/service_btn_padding"
                android:paddingTop="5dp"
                android:singleLine="true"
                android:text="@string/serviceticket"
                android:textColor="@color/lb_tv_white"
                android:textSize="18sp" />
        </LinearLayout>


    </RelativeLayout>
</RelativeLayout>